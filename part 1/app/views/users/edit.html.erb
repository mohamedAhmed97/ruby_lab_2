<h1>Edit User</h1>
<%= form_with(model: @user, local: true) do |form| %>
 
  <p>
    <%= form.label :username %><br>
    <%= form.text_field :username %>
  </p>
 
  <p>
    <%= form.label :email %><br>
    <%= form.text_area :email %>
  </p>
  
 <p>
    <%= form.submit %>
  </p>
 
<% end %>
 
<%= link_to 'Back', users_path %>
